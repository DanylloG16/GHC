<?xml version="1.0" encoding="UTF-8"?>




<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
         
         
         
         

    <!-- Configuração do Servlet para envio do certificado -->
    <servlet>
        <servlet-name>controller.EnvioCertificadoController</servlet-name>
        <servlet-class>EnvioCertificadoController</servlet-class>
    </servlet>

    <!-- Mapeamento da URL para o Servlet -->
    <servlet-mapping>
        <servlet-name>controller.EnvioCertificadoController</servlet-name>
        <url-pattern>/envio-certificados</url-pattern>
    </servlet-mapping>

    

</web-app>
